    @code{

    [Inject]
    private IJSRuntime jsRuntime { get; set; }

    private async void ScrollUp()
    {
        var jsModule = await jsRuntime.InvokeAsync<IJSObjectReference>("import", "./js/scrollup.js");
        await jsModule.InvokeVoidAsync("scrollToTop");
    }


